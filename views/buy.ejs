        <section class="header-back _header-padding">
			<div class="container-fluid">
				<div class="card border-0 rounded-2 p-4">
				  	<div class="card-body p-0">
						<form action="/buy" method="POST" class="form row">
							<div class="col-md-2 ordr2">
								<select name="select1" class="form-select select-header" aria-label="Default select example">
									<option selected>Open this select menu</option>
									<option value="1">One</option>
									<option value="2">Two</option>
									<option value="3">Three</option>
							  </select>
							</div>
							<div class="col-md-2 ordr3">
								<select name="select2" class="form-select select-header" aria-label="Default select example">
									<option selected>Open this select menu</option>
									<option value="1">One</option>
									<option value="2">Two</option>
									<option value="3">Three</option>
							  </select>
							</div>
							<div class="col-md-4 ordr1">
								<div class="input-group mb-md-3">
								    <input name="receip" type="text" class="form-control header-input" placeholder="Recipient's username">
							    </div>
							</div>
							<div class="col-md-2 ordr4">
								<select name="select3" class="form-select select-header" aria-label="Default select example">
									<option selected>Open this select menu</option>
									<option value="1">One</option>
									<option value="2">Two</option>
									<option value="3">Three</option>
							  </select>
							</div>
							<div class="col-md-2 ordr5">
								<select name="select4" class="form-select select-header" aria-label="Default select example">
									<option selected>Open this select menu</option>
									<option value="1">One</option>
									<option value="2">Two</option>
									<option value="3">Three</option>
							  </select>
							</div>
							<div class="d-flex justify-content-center align-items-center flex-column ordr6">
								<button class="btn-primary px-5">Search</button>
							</div>
						</form>
				  	</div>
				</div>
			</div>
		</section>
